<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200" prefWidth="300" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManageConfirmBoxController">
   <children>
      <Button layoutX="45.0" layoutY="196.0" mnemonicParsing="false" onAction="#deleteOrderConformation" text="Delete">
         <font>
            <Font name="Inconsolata" size="13.0" />
         </font></Button>
      <Button layoutX="233.0" layoutY="196.0" mnemonicParsing="false" onAction="#cancel" text="Cancel">
         <font>
            <Font name="Inconsolata" size="13.0" />
         </font></Button>
      <Text fill="#b91e1e" layoutX="58.0" layoutY="172.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Once you delete an order, there is no going back. Please be certain." textAlignment="CENTER" wrappingWidth="239.21875">
